<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_latest_news_details"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.rrmsense.enewspaperonline.TabLatest.LatestNewsDetails">

    <WebView
        android:id="@+id/webview_new"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">

        <ProgressBar
            android:id="@+id/progressBar_new"
            style="?android:attr/progressBarStyleLarge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="@dimen/activity_horizontal_margin"
            android:layout_y="@dimen/activity_vertical_margin" />
    </WebView>

</RelativeLayout>
